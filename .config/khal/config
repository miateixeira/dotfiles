[calendars]

[[private]]
path = /home/mia/.local/share/khal/calendars/private
type = calendar
color = white

[locale]
timeformat = %H:%M
dateformat = "%b %d, %Y"
longdateformat = "%b %d, %Y"
datetimeformat = "%b %d, %Y at %H:%M"
longdatetimeformat = "%b %d, %Y at %H:%M"

[default]
highlight_event_days = True

[view]
frame = width
event_view_always_visible = True

[highlight_days]
color = dark gray
